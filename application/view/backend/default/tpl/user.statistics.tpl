[include "user.tabs"]

<div class="panel panel-primary">
	<div class="panel-heading">
		<h3 class="panel-title">[f:user:statistics.users]</h3>
	</div>

	<table class="table">
		<tbody>
		<tr>
			<td>[f:user:statistics.users.num]</td>
			<td>{users-num}</td>
		</tr>
		<tr>
			<td>[f:user:statistics.users.groups]</td>
			<td>{users-groups}</td>
		</tr>
		<tr>
			<td>[f:user:statistics.users.sessions]</td>
			<td>{users-sessions}</td>
		</tr>
		</tbody>
	</table>
</div>
